#Simulate worst points and evaluate objective function
define evalworst
    opt worst
    evalId = ans
    call setpars
    sim
    call obj
enddefine


#Simulate to get initial objective values
define evalall
    i=0
    while (i<npoints)
        evalId = i
        call setpars
        sim
        call obj
        i=i+1
    repeat
enddefine
