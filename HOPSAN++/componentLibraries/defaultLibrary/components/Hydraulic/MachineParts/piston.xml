<?xml version="1.0" encoding="UTF-8"?>
<hopsanobjectappearance version="0.3">
    <modelobject typename="HydraulicPumpPiston" displayname="Piston">
        <icons>
            <icon scale="1" path="svg/piston.svg" iconrotation="ON" type="user"/>
        </icons>
        <ports>
            <port x="0.5" y="1.0" a="270" name="P1"/>
            <port x="0.5" y="0.0" a="90" name="P3"/>
        </ports>
        <animation>
            <icon userpath="svg/piston_base.svg"/>
            <movable>
                <icon userpath="svg/piston_movable_2.svg"/>
                <data port="P3" dataname="Position"/> 
                <data port="P1" dataname="Pressure"/>
                <divisor name="s_l"/>
                <movement x="0" y="0" a="0"/>
                <start x="1" y="21" a="0"/> 
                <initscale x="1" y="1"/>
                <initcolor r="0" g="0" b="255"/>
                <color r="-0.00001275" g="0" b="0.00001275" idx="1"/>
                <transformorigin x="0" y="0"/>
		        <movingport portname="P3" startx="100" starty="20"/>
            </movable>
            <movable>
                <icon userpath="svg/piston_movable.svg"/>
                <data port="P3" dataname="Position"/> 
                <divisor name="s_l"/>
                <movement x="0" y="-55" a="0"/>
                <start x="0" y="-45" a="0"/>
                <transformorigin x="0" y="0"/>
		        <movingport portname="P3" startx="10" starty="10"/>
            </movable>
            <movable>
                <icon userpath="svg/piston_base.svg"/>
                <data port="P3" dataname="Position"/> 
                <movement x="0" y="0" a="0"/>
                <start x="0" y="0" a="0"/>
                <transformorigin x="0" y="0"/>
            </movable>
        </animation>
    </modelobject>
</hopsanobjectappearance>
